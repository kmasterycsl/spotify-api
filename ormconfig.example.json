{
  "type": "mysql",
  "host": "MYSQL_HOST",
  "port": MYSQL_PORT,
  "username": "MYSQL_USER",
  "password": "MYSQL_PASSWORD",
  "database": "MYSQL_DB",
  "entities": [
    "dist/**/*.entity{.ts,.js}"
  ],
  "synchronize": true,
  "factories": [
    "src/database/factories/**/*{.ts,.js}"
  ],
  "seeds": [
    "src/database/seeds/**/*{.ts,.js}"
  ]
}